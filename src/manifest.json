{
  "name": "Video Play back controls",
  "version": "0.1",
  "description": "Speed up, slow down, skip around",
  "permissions": ["activeTab", "storage"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["index.js"]
    }
  ],
  "options_page": "options/options.html"
}
